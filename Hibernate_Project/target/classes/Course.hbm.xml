<?xml version="1.0" encoding="UTF-8"?>
 
 
 <!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 

<hibernate-mapping>
	<class name="example.hibernate.Course" 
		table="course_master">
		<id name="courseId" type="int" column="course_Id"/>
		<property name="title" type="string" column="course_title"/>
		<property name="duration" type="int" column="course_duration"/>
		<property name="fees" type="int" column="course_fees" />				
	</class>
</hibernate-mapping>